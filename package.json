    "@mui/x-date-pickers": "^8.9.2",
    "dayjs": "^1.11.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@eslint/js": "^9.30.1",
    "@storybook/addon-a11y": "^9.1.1",
